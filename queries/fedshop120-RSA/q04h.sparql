PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

SELECT DISTINCT ?product ?label ?propertyTextual WHERE {
    VALUES ( ?bgp1 ?bgp2 ) { ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite28.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.vendor34.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite32.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite4.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite28.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.vendor34.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite32.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite4.fr> ) }
    {
        SERVICE ?bgp1 { 
            ?product rdfs:label ?label .
            # const!* bsbm:ProductType593
            ?product rdf:type ?localProductType .
            ?localProductType owl:sameAs bsbm:ProductType593 .
            # const!* bsbm:ProductFeature20512
            ?product bsbm:productFeature ?localProductFeature1 .
            ?localProductFeature1 owl:sameAs bsbm:ProductFeature20512.
            # const** bsbm:ProductFeature20416 != bsbm:ProductFeature20512
            ?product bsbm:productFeature ?localProductFeature2 .
            ?localProductFeature2 owl:sameAs bsbm:ProductFeature20416.
            ?product bsbm:productPropertyTextual1 ?propertyTextual .
            ?product bsbm:productPropertyNumeric1 ?p1 .
            # const** "1327.0"^^xsd:double < ?p1
            FILTER ( ?p1 > "1327.0"^^xsd:double )
        } 
    } UNION {
        SERVICE ?bgp2 {
            ?product rdfs:label ?label .
            # const!* bsbm:ProductType593
            ?product rdf:type ?localProductType .
            ?localProductType owl:sameAs bsbm:ProductType593 .
            # const!* bsbm:ProductFeature20512
            ?product bsbm:productFeature ?localProductFeature1 .
            ?localProductFeature1 owl:sameAs bsbm:ProductFeature20512 .
            # const* bsbm:ProductFeature14005 != bsbm:ProductFeature20416, bsbm:ProductFeature20512
            ?product bsbm:productFeature ?localProductFeature3 .
            ?localProductFeature3 owl:sameAs bsbm:ProductFeature14005 .
            ?product bsbm:productPropertyTextual1 ?propertyTextual .
            ?product bsbm:productPropertyNumeric2 ?p2 .
            # const "1218.0"^^xsd:double < ?p2
            FILTER ( ?p2 > "1218.0"^^xsd:double ) 
        } 
    }
}
ORDER BY ?product ?label ?propertyTextual
##OFFSET 5
LIMIT 10