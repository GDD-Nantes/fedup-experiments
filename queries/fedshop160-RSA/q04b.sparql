PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

SELECT DISTINCT ?product ?label ?propertyTextual WHERE {
    VALUES ( ?bgp1 ?bgp2 ) { ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite10.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite4.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite11.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite79.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.vendor59.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite10.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite4.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite11.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.vendor59.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite79.fr> ) }
    {
        SERVICE ?bgp1 { 
            ?product rdfs:label ?label .
            # const!* bsbm:ProductType398
            ?product rdf:type ?localProductType .
            ?localProductType owl:sameAs bsbm:ProductType398 .
            # const!* bsbm:ProductFeature18986
            ?product bsbm:productFeature ?localProductFeature1 .
            ?localProductFeature1 owl:sameAs bsbm:ProductFeature18986.
            # const** bsbm:ProductFeature24424 != bsbm:ProductFeature18986
            ?product bsbm:productFeature ?localProductFeature2 .
            ?localProductFeature2 owl:sameAs bsbm:ProductFeature24424.
            ?product bsbm:productPropertyTextual1 ?propertyTextual .
            ?product bsbm:productPropertyNumeric1 ?p1 .
            # const** "1036.0"^^xsd:double < ?p1
            FILTER ( ?p1 > "1036.0"^^xsd:double )
        } 
    } UNION {
        SERVICE ?bgp2 {
            ?product rdfs:label ?label .
            # const!* bsbm:ProductType398
            ?product rdf:type ?localProductType .
            ?localProductType owl:sameAs bsbm:ProductType398 .
            # const!* bsbm:ProductFeature18986
            ?product bsbm:productFeature ?localProductFeature1 .
            ?localProductFeature1 owl:sameAs bsbm:ProductFeature18986 .
            # const* bsbm:ProductFeature19400 != bsbm:ProductFeature24424, bsbm:ProductFeature18986
            ?product bsbm:productFeature ?localProductFeature3 .
            ?localProductFeature3 owl:sameAs bsbm:ProductFeature19400 .
            ?product bsbm:productPropertyTextual1 ?propertyTextual .
            ?product bsbm:productPropertyNumeric2 ?p2 .
            # const "854.0"^^xsd:double < ?p2
            FILTER ( ?p2 > "854.0"^^xsd:double ) 
        } 
    }
}
ORDER BY ?product ?label ?propertyTextual
##OFFSET 5
LIMIT 10