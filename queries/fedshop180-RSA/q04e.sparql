PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

SELECT DISTINCT ?product ?label ?propertyTextual WHERE {
    VALUES ( ?bgp1 ?bgp2 ) { ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite52.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite67.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.vendor41.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite66.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite4.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite45.fr> <http://localhost:8890/sparql?default-graph-uri=nan> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite52.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite45.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite67.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.vendor41.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite66.fr> ) ( <http://localhost:8890/sparql?default-graph-uri=nan> <http://localhost:8890/sparql?default-graph-uri=http://www.ratingsite4.fr> ) }
    {
        SERVICE ?bgp1 { 
            ?product rdfs:label ?label .
            # const!* bsbm:ProductType544
            ?product rdf:type ?localProductType .
            ?localProductType owl:sameAs bsbm:ProductType544 .
            # const!* bsbm:ProductFeature16299
            ?product bsbm:productFeature ?localProductFeature1 .
            ?localProductFeature1 owl:sameAs bsbm:ProductFeature16299.
            # const** bsbm:ProductFeature16765 != bsbm:ProductFeature16299
            ?product bsbm:productFeature ?localProductFeature2 .
            ?localProductFeature2 owl:sameAs bsbm:ProductFeature16765.
            ?product bsbm:productPropertyTextual1 ?propertyTextual .
            ?product bsbm:productPropertyNumeric1 ?p1 .
            # const** "1668.0"^^xsd:double < ?p1
            FILTER ( ?p1 > "1668.0"^^xsd:double )
        } 
    } UNION {
        SERVICE ?bgp2 {
            ?product rdfs:label ?label .
            # const!* bsbm:ProductType544
            ?product rdf:type ?localProductType .
            ?localProductType owl:sameAs bsbm:ProductType544 .
            # const!* bsbm:ProductFeature16299
            ?product bsbm:productFeature ?localProductFeature1 .
            ?localProductFeature1 owl:sameAs bsbm:ProductFeature16299 .
            # const* bsbm:ProductFeature22074 != bsbm:ProductFeature16765, bsbm:ProductFeature16299
            ?product bsbm:productFeature ?localProductFeature3 .
            ?localProductFeature3 owl:sameAs bsbm:ProductFeature22074 .
            ?product bsbm:productPropertyTextual1 ?propertyTextual .
            ?product bsbm:productPropertyNumeric2 ?p2 .
            # const "402.0"^^xsd:double < ?p2
            FILTER ( ?p2 > "402.0"^^xsd:double ) 
        } 
    }
}
ORDER BY ?product ?label ?propertyTextual
##OFFSET 5
LIMIT 10